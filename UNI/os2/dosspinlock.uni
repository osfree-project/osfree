   #define PROC_ONLINE     0x00000001      /* processor is online */

   APIRET APIENTRY DosGetProcessorStatus(ULONG procid,
                                         PULONG status);

   APIRET APIENTRY DosSetProcessorStatus(ULONG procid,
                                         ULONG status);

