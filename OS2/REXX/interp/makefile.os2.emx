	       $(CC) $(COPT) $(CC2O) -c $(SRCDIR)/error.c

variable.$(OBJ) :$(SRCDIR)/variable.c    $(SRCDIR)/rexx.h
	       $(CC) $(COPT) $(CC2O) -c $(SRCDIR)/variable.c

interprt.$(OBJ) :$(SRCDIR)/interprt.c    $(SRCDIR)/rexx.h
	       $(CC) $(COPT) $(CC2O) -c $(SRCDIR)/interprt.c

