	       $(CC) $(COPT) $(CC2O) -c $(SRCDIR)/funcs.c

builtin.$(OBJ) : $(SRCDIR)/builtin.c     $(SRCDIR)/rexx.h
	       $(CC) $(COPT) $(CC2O) -c $(SRCDIR)/builtin.c

